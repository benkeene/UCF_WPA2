[connection]
id=UCF_WPA2
uuid=//ENTER UUID FROM 'uuidgen' HERE//
type=wifi
permissions=

[wifi]
mac-address-blacklist=
mode=infrastructure
ssid=UCF_WPA2

[wifi-security]
auth-alg=open
key-mgmt=wpa-eap

[802-1x]
eap=peap;
identity=//ENTER nID HERE e.g., ab123456//
password=//ENTER PASSWORD HERE//
phase2-auth=mschapv2

[ipv4]
dns-search=
method=auto

[ipv6]
addr-gen-mode=eui64
dns-search=
method=auto

[proxy]
